{
  "name": "edge-flags",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "fmt": "rome check . --apply-suggested && rome format . --write"
  },
  "devDependencies": {
    "rome": "^10.0.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.12.1"
}