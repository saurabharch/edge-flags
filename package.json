{
  "name": "@upstash/edge-flags",
  "version": "0.0.0-rc.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "fmt": "pnpm rome check . --apply-suggested && pnpm rome format . --write",
    "release": "pnpm fmt && pnpm build && pnpx changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "rome": "^10.0.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.12.1"
}